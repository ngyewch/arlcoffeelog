{
  "name": "arlcoffeelog",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "run-p watch:* doDev",
    "build": "run-s check doBuild",
    "doDev": "vite",
    "doBuild": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig tsconfig.app.json && tsc -p tsconfig.node.json",
    "watch:svelte-check": "svelte-check --tsconfig tsconfig.app.json --watch",
    "watch:tsc": "tsc -p tsconfig.node.json --watch",
    "deploy": "run-s build doDeploy",
    "doDeploy": "gh-pages -d dist --nojekyll"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "@tsconfig/svelte": "5.0.4",
    "@types/netlify-auth-providers": "1.0.2",
    "gh-pages": "6.3.0",
    "netlify-cli": "21.2.1",
    "npm-run-all2": "8.0.1",
    "svelte-check": "4.1.7",
    "typescript": "5.8.3",
    "vite": "6.3.5"
  },
  "dependencies": {
    "@jill64/svelte-storage": "2.0.1",
    "@octokit/rest": "21.1.1",
    "@picocss/pico": "2.1.1",
    "base64-js": "1.5.1",
    "date-fns": "4.1.0",
    "ky": "1.8.1",
    "netlify-auth-providers": "1.0.0-alpha5",
    "sgqr": "0.8.2",
    "sweetalert2": "11.21.0",
    "svelte": "5.28.2"
  }
}
